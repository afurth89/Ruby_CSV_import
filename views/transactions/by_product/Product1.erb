<% @transactions.each do |t| %>
<tr>
<td><%= t.transaction_date %></td>
<td><%= t.product %></td>
<td><%= t.price %></td>
<td><%= t.payment_type %></td>
<td><%= t.name %></td>
<td><%= t.city %></td>
<td><%= t.state %></td>
<td><%= t.country %></td>
<td><%= t.account_created %></td>
<td><%= t.last_login %></td>
</tr>
<% end %>